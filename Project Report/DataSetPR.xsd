                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="true">
                    <CommandText>SELECT ID,Name FROM (
SELECT        "AIT$USERS"."AIT$USER_REALNAME" AS Name, "AIT$USERS"."AIT$USER_ID" AS ID
FROM            "AIT$USERS" LEFT OUTER JOIN
